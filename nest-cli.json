{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/knyharnya/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/knyharnya/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/knyharnya",
  "projects": {
    "knyharnya": {
      "type": "application",
      "root": "apps/knyharnya",
      "entryFile": "main",
      "sourceRoot": "apps/knyharnya/src",
      "compilerOptions": {
        "tsConfigPath": "apps/knyharnya/tsconfig.app.json"
      }
    },
    "orders": {
      "type": "application",
      "root": "apps/orders",
      "entryFile": "main",
      "sourceRoot": "apps/orders/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orders/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "storage": {
      "type": "application",
      "root": "apps/storage",
      "entryFile": "main",
      "sourceRoot": "apps/storage/src",
      "compilerOptions": {
        "tsConfigPath": "apps/storage/tsconfig.app.json"
      }
    }
  }
}